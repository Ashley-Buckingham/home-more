<head>
  <title>Checkout</title>
  <script src="https://js.stripe.com/v3/"></script>
</head>

<script>
  const stripe = Stripe("<%= publicKey %>");
  const elements = stripe.elements();

  stripe
    .redirectToCheckout({
      sessionId: "<%= CHECKOUT_SESSION_ID %>",
    })
    .then(function (result) {
      alert(result.error.message);
    });
</script>
